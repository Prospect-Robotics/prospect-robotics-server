{
  "name": "prospect-website-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-express": "node ./bin/www --dev",
    "start": "node ./bin/www",
    "start-react": "set PORT=3001 && react-scripts start",
    "dev": "set PORT=3001 concurrently --kill-others \"npm run start-react\" \"npm run start-express\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "antd": "^3.16.6",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-fileupload": "^1.1.4",
    "express-http-proxy": "^1.5.1",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "https://localhost:3000/",
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
